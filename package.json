{
  "name": "belugasswimming-client",
  "version": "1.0.0",
  "author": "https://digitaltechnologia.com",
  "private": true,
  "scripts": {
    "dev": "concurrently 'next dev' 'yarn tailwind --watch'",
    "tailwind": "tailwindcss -i src/styles/tailwind.css -o build/styles/tailwind.css --minify",
    "build": "tailwindcss -i src/styles/tailwind.css -o build/styles/tailwind.css --minify && next build",
    "export": "tailwindcss -i src/styles/tailwind.css -o build/styles/tailwind.css --minify && next build && next export",
    "start": "next start",
    "analyze": "ANALYZE=true next build",
    "lint": "next lint",
    "code-check": "npx eslint . --ext .ts --ext .tsx --fix",
    "format": "npx prettier --write \"**/*.+(html|css|scss|sass|less|js|jsx|ts|tsx|json|php)\"",
    "prepare": "husky install"
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@mui/material": "^5.11.0",
    "@next/bundle-analyzer": "^13.0.7",
    "@reduxjs/toolkit": "^1.9.1",
    "@types/node": "18.11.16",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.9",
    "eslint": "8.30.0",
    "eslint-config-next": "13.0.7",
    "gsap": "^3.11.3",
    "libphonenumber-js": "^1.10.15",
    "next": "13.0.7",
    "next-seo": "^5.15.0",
    "react": "18.2.0",
    "react-calendar": "^4.0.0",
    "react-dom": "18.2.0",
    "react-icons": "^4.7.1",
    "react-lottie": "^1.2.3",
    "react-redux": "^8.0.5",
    "sharp": "^0.31.2",
    "simplebar-react": "^2.4.3",
    "swiper": "^8.4.5",
    "typescript": "4.9.4"
  },
  "devDependencies": {
    "@svgr/webpack": "^6.5.1",
    "@types/react-calendar": "^3.9.0",
    "@types/react-lottie": "^1.2.6",
    "@types/react-redux": "^7.1.24",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "chokidar-cli": "^3.0.0",
    "concurrently": "^7.6.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-standard-with-typescript": "^24.0.0",
    "eslint-plugin-tailwindcss": "^3.7.1",
    "husky": "^8.0.2",
    "prettier": "^2.8.1",
    "prettier-plugin-tailwindcss": "^0.2.1",
    "sass": "^1.57.0",
    "tailwindcss": "^3.2.4"
  }
}
