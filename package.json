{
  "name": "my-iclinic",
  "version": "1.0.0",
  "author": "https://digitaltechnologia.com",
  "private": true,
  "scripts": {
    "dev": "concurrently \"next dev\" \"pnpm tailwind --watch\"",
    "tailwind": "tailwindcss -i src/styles/tailwind.css -o build/styles/tailwind.css --minify",
    "build": "tailwindcss -i src/styles/tailwind.css -o build/styles/tailwind.css --minify && next build && next-sitemap",
    "export": "tailwindcss -i src/styles/tailwind.css -o build/styles/tailwind.css --minify && next build && next export",
    "start": "next start",
    "analyze": "ANALYZE=true next build",
    "lint": "next lint",
    "code-check": "npx eslint . --ext .ts --ext .tsx --fix && npx tsc",
    "format": "npx prettier --write \"**/*.+(html|css|scss|sass|less|js|jsx|ts|tsx|json|php)\"",
    "prepare": "husky install",
    "postbuild": "npm run algolia",
    "algolia": "node algolia/articles.js"
  },
  "dependencies": {
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@img-comparison-slider/react": "^8.0.0",
    "@mui/material": "^5.11.10",
    "@react-buddy/ide-toolbox-next": "^2.4.1",
    "@react-buddy/palette-mui": "^5.0.1",
    "@reduxjs/toolkit": "^1.9.3",
    "@tailwindcss/line-clamp": "^0.4.2",
    "@wordpress/block-library": "^8.12.0",
    "algoliasearch": "^4.18.0",
    "axios": "^1.6.2",
    "cheerio": "1.0.0-rc.12",
    "dotenv": "^16.3.1",
    "gsap": "^3.11.4",
    "html-react-parser": "^3.0.9",
    "js-search": "^2.0.0",
    "jsdom": "^24.0.0",
    "libphonenumber-js": "^1.10.21",
    "next": "^14.2.3",
    "next-sitemap": "^4.0.2",
    "plyr": "^3.7.3",
    "react": "18.2.0",
    "react-calendar": "^4.0.0",
    "react-dom": "18.2.0",
    "react-icons": "^4.7.1",
    "react-instantsearch-dom": "^6.40.1",
    "react-lottie": "^1.2.3",
    "sharp": "^0.31.3",
    "striptags": "^3.2.0",
    "swiper": "^8.4.7",
    "swr": "^2.2.4",
    "tailwind-merge": "^2.2.2",
    "tailwindcss": "^3.4.3",
    "typescript": "4.9.5",
    "valtio": "^1.13.1"
  },
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@next/bundle-analyzer": "^13.2.1",
    "@types/html-escaper": "^3.0.0",
    "@types/js-search": "^1.4.0",
    "@types/jsdom": "^21.1.5",
    "@types/node": "18.14.1",
    "@types/react": "18.0.28",
    "@types/react-animated-modal": "^1.1.1",
    "@types/react-calendar": "^3.9.0",
    "@types/react-dom": "18.0.11",
    "@types/react-gtm-module": "^2.0.1",
    "@types/react-html-parser": "^2.0.2",
    "@types/react-instantsearch-dom": "^6.12.3",
    "@types/react-lottie": "^1.2.6",
    "@types/react-redux": "^7.1.25",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "concurrently": "^7.6.0",
    "eslint": "8.34.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-next": "14.1.0",
    "eslint-config-standard-with-typescript": "^34.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-tailwindcss": "^3.9.0",
    "husky": "^8.0.3",
    "prettier": "^2.8.4",
    "prettier-eslint": "^15.0.1",
    "prettier-plugin-tailwindcss": "^0.2.3",
    "sass": "^1.58.3"
  }
}
